<Window x:Class="Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ed="clr-namespace:Editor"
        Title="MainWindow" SizeToContent="Height" Width="500">
    <StackPanel Orientation="Vertical">
        <MediaElement Name="FaceVideo"></MediaElement>
        <ed:Timeline x:Name="Timeline"/>
        <CheckBox Name="OnlyGood">Проигрывать только одобренные фрагменты</CheckBox>
    </StackPanel>
</Window>
